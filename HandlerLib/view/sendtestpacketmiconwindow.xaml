<Window x:Class="HandlerLib.SendTestPacketMiConWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Send Test Packet (MinoConnect)" Height="120" Width="445" WindowStartupLocation="CenterOwner"
        Icon="pack://application:,,,/StartupLib;component/Images/ZR.ico">
  <Grid>
    <Button Click="ButtonConnectionProfile_Click" x:Name="ButtonConnectionProfile" Content="Connection Profile"
            HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="120"/>
    <Button Click="ButtonSend_Click" x:Name="ButtonSend" Content="Send" Margin="0,10,10,0"
            VerticalAlignment="Top" HorizontalAlignment="Right" Width="75"/>
    <Label Content="Serialnumber:" HorizontalAlignment="Left" Margin="149,8,0,0" VerticalAlignment="Top"
           RenderTransformOrigin="0,0.115"/>
    <TextBox x:Name="TextBoxSerialnumber" HorizontalAlignment="Left" Height="23" Margin="234,10,0,0"
             TextWrapping="Wrap" Text="11111111" VerticalAlignment="Top" Width="69"/>
    <Label Content="Mode:" HorizontalAlignment="Left" Margin="149,34,0,0" VerticalAlignment="Top"
           RenderTransformOrigin="0,0.115"/>
    <ComboBox Name="ComboBoxMode" HorizontalAlignment="Left" Margin="193,37,0,0" VerticalAlignment="Top"
              Width="140"/>
  </Grid>
</Window>