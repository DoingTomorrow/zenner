<Application x:Class="MSS_Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Converters="clr-namespace:MVVM.Converters;assembly=MVVM"
             xmlns:Localisation="clr-namespace:MSS.Localisation;assembly=MSS.Localisation"
             xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
      </ResourceDictionary.MergedDictionaries>
      <ObjectDataProvider x:Key="Resources" MethodName="GetResourceInstance"
                          ObjectType="{Type Localisation:CultureResources}"/>
      <Converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
      <Converters:JobStatusToBoolean x:Key="JobStatusToBoolean"/>
      <Converters:JobStartedStatusToBoolean x:Key="JobStartedStatusToBoolean"/>
      <Converters:SelectedItemsToVisibilityConverter x:Key="SelectedItemsToVisibilityConverter"/>
      <Converters:IsNullConverter x:Key="IsNullConverter"/>
      <Converters:IsNotNullConverter x:Key="IsNotNullConverter"/>
      <Converters:IsNotNullOrFalseConverter x:Key="IsNotNullOrFalseConverter"/>
      <Converters:IsOneSelectedItemToVisibilityConverter x:Key="IsOneSelectedItemToVisibilityConverter"/>
      <Converters:BuildingNumberConverter x:Key="BuildingNumberConverter"/>
      <Converters:BoolToSelectionModeConverter x:Key="BoolToSelectionModeConverter"/>
      <Converters:InverseBooleanConverter x:Key="InverseBooleanConverter"/>
      <Converters:CustomMultiBindingConverter x:Key="CustomMultiBindingConverter"/>
      <Converters:StructureNodeTranslationConverter x:Key="StructureNodeTranslationConverter"/>
      <Converters:NodeToIsMinomatMasterConverter x:Key="NodeToIsMinomatMasterConverter"/>
      <Converters:NodeToIsMinomatConverter x:Key="NodeToIsMinomatConverter"/>
      <Converters:NodeToAssignMetersEnabledConverter x:Key="NodeToAssignMetersEnabledConverter"/>
      <Converters:NodeToTestGSMEnabledConverter x:Key="NodeToTestGSMEnabledConverter"/>
      <Converters:RowToIndexConverter x:Key="RowToIndexConverter"/>
      <Converters:IsNullOrEmptyConverter x:Key="IsNullOrEmptyConverter"/>
      <Converters:RoomCodeToStringConverter x:Key="RoomCodeToStringConverter"/>
      <Converters:MandatoryValueLabelConverter x:Key="MandatoryValueLabelConverter"/>
      <Converters:GSMStatusToStringConverter x:Key="GSMStatusToStringConverter"/>
    </ResourceDictionary>
  </Application.Resources>
</Application>